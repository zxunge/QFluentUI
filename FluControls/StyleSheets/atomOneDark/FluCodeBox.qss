FluCodeBox {
    background-color: rgb(44, 48, 54);
    border: 1px solid rgb(71, 74, 79);
    font-size: 14px;
    border-radius: 4px;
    selection-background-color: rgb(62, 68, 81);
    selection-color: rgb(255, 255, 255);
    color: white;
}

FluCodeBox[transparent=true] {
    background-color: transparent;
    font-size: 14px;
    border: none;
}